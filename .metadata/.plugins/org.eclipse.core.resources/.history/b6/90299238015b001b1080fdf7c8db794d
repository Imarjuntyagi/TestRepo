public class aa {
 
	public static void main(String[] args) {
		
		int n=1;
		int a=1;
		
		if(n==1)
		{
			System.out.println("Fact is 1");
		
	}
		else
		{
			for (int i=n;i>=1;i--)
			{
				a=a*i;
				
			}
		}
		System.out.println(a);
		}
	
}
	
	

	